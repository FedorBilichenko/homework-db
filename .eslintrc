{
  "extends": [
    "plugin:@shopify/typescript",
    "plugin:@shopify/react",
    "plugin:@shopify/prettier",
    "plugin:@shopify/typescript-type-checking"
  ],
  "parserOptions": {
    "project": "tsconfig.json"
  },
  "rules": {
    "no-unused-vars": "error",
    "no-unused-expressions": "error"
  }
}